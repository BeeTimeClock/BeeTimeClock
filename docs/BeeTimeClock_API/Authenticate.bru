meta {
  name: Authenticate
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/api/v1/auth?Username={{username}}&Password={{password}}
  body: json
  auth: none
}

params:query {
  Username: {{username}}
  Password: {{password}}
}

script:post-response {
  bru.setEnvVar('token', res.body.Data.Token)
}
